<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="dm_arm">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import dm_arm urdf file -->
    <xacro:include filename="$(find dm_robotarm_description)/urdf/dm_arm.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="dm_arm.ros2_control.xacro" />


    <xacro:dm_arm_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
